POSTGRES_DB=checkout
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_PORT=5432
POSTGRES_CONTAINER=checkout-postgres
PGUSER=$POSTGRES_USER

POSTGRESQL_USERNAME=$POSTGRES_USER
POSTGRESQL_PASSWORD=$POSTGRES_PASSWORD
POSTGRESQL_DATABASE=$POSTGRES_DB

PGBOUNCER_CONTAINER=checkout-pgbouncer
PGBOUNCER_DATABASE=$POSTGRES_DB
POSTGRESQL_HOST=$POSTGRES_CONTAINER
PGBOUNCER_PORT=6432

DB_DSN=postgres://$POSTGRES_USER:$POSTGRES_PASSWORD@$PGBOUNCER_CONTAINER:$PGBOUNCER_PORT/$POSTGRES_DB?sslmode=disable